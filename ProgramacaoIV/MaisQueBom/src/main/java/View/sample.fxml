<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.202-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <BorderPane prefHeight="600.0" prefWidth="600.0">
         <top>
            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="Admin">
                  <items>
                    <MenuItem fx:id="SubMenuadminLoginHome" mnemonicParsing="false" onAction="#SubMenuLoginAdmin" text="Login Admin" />
                        <MenuItem fx:id="SubMenuExitAdminHome" mnemonicParsing="false" onAction="#SairApp" text="Exit" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Funcionário">
                  <items>
                    <MenuItem fx:id="SubMenuFuncLoginHome" onAction="#BtnFuncHome" mnemonicParsing="false" text="Login Funcionário" />
                        <MenuItem fx:id="SubMenuExitFuncHome" mnemonicParsing="false" onAction="#SairAppFunc" text="Exit" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <left>
            <AnchorPane prefHeight="571.0" prefWidth="294.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btnAdminHome" layoutX="58.0" layoutY="211.0" mnemonicParsing="false" onAction="#BtnAdminHome" prefHeight="150.0" prefWidth="179.0" text="Admin" />
               </children>
            </AnchorPane>
         </left>
         <right>
            <AnchorPane prefHeight="571.0" prefWidth="304.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btnFuncHome" onAction="#BtnFuncHome" layoutX="63.0" layoutY="211.0" mnemonicParsing="false" prefHeight="150.0" prefWidth="179.0" text="Funcionário" />
               </children>
            </AnchorPane>
         </right>
      </BorderPane>
   </children>
</GridPane>
